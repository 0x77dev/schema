  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug SWAPI Api",
      "protocol": "inspector",
      "cwd": "${workspaceRoot}/examples/star-wars",
      "runtimeExecutable": "${workspaceRoot}/examples/star-wars/node_modules/.bin/ts-node-dev",
      "args": [
        "--no-notify",
        "--transpileOnly",
        "${workspaceRoot}/examples/star-wars/src/index.ts"
      ],
      "restart": true,
      "sourceMaps": true
    }
  ]
}
